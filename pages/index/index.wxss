page{
  height: 100%;
  width: 100%;
  background-color: cornsilk;
}
.grid {
  font-family:Arial,Helvetica,sans-serif;
  font-weight: bold;
  font-size: 36rpx;
  margin: 20rpx 50rpx 20rpx 50rpx;
  border-radius: 40rpx;
  padding: 20rpx;
  background-color: white;
}
.grid image{
  width: 66rpx;
  height: 66rpx;
}
.my-swipe{
  margin-top: 8rpx;
  margin-left: 8rpx;
  margin-right: 8rpx;
}
.my-swipe swiper{
  height: 228px;
}
.my-swipe image{
  border-radius: 30rpx;
  width: 100%;
  height: 100%;
}
