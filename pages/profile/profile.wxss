page{
  background-color: #EEEEEE;
}
.login {
  position: relative;
  display: flex;
  align-items: center;
  width: 100%;
  height: 180rpx;
  padding: 0 40rpx;
  background-color: #F69C0A;
  margin-top: 3rpx;
}

.user-pic {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background-color: white;
}

.user-pic .user-pic-img {
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 100rpx;
}

.user-info {
  font-size: 32rpx;
  margin-left: 40rpx;
  color: white;
}

.user-info p:last-child {
  margin-left: 6rpx;
}

.user-info .phone-num {
  font-size: 28rpx;
  display: flex;
  align-items: center;
  margin-top: 16rpx;
}

.icon-phone {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
}

.profile-arrow-right {
  position: absolute;
  right: 120rpx;
  width: 40rpx;
  height: 40rpx;
}

.profile-arrow-right image {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
}
.money {
  display: flex;
  width: 95%;
  margin: auto;
  background-color: white;
  border-radius: 20rpx;
  margin-top: 10rpx;
}

.money .money-item {
  display: flex;
  align-items: center;
  flex: 1;
  flex-direction: column;
  justify-content: center;
  height: 170rpx;
}

.money .money-item:nth-of-type(1),
.money .money-item:nth-of-type(2) {
  border-right: 2rpx solid #eeeeee;
}

.money .money-item p:first-child {
  font-size: 48rpx;
  font-weight: 700;
  margin-bottom: 6rpx;
  color: #ff5f3e;
}

.money .money-item p:first-child span {
  font-size: 28rpx;
  font-weight: normal;
  margin-left: 6rpx;
  color: #666666;
}

.money .money-item p:last-child {
  font-size: 28rpx;
  color: #666666;
}

.cell{
  margin-top: 20rpx;
}